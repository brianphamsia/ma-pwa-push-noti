{
	"useTabs": true,
	"tabWidth": 4,
	"jsxSingleQuote": true,
	"singleQuote": true,
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"proseWrap": "preserve",
	"requirePragma": false,
	"semi": true,
	"singleAttributePerLine": false,
	"trailingComma": "all",
	"vueIndentScriptAndStyle": false,
	"printWidth": 160
}
